{
    "manifest_version": 3,
    "name": "Chatbot Extension",
    "version": "1.0",
    "description": "A simple chatbot extension",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "48": "scraper.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["popup.js"]
        }
    ]
}